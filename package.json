{
  "name": "STrack",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "clean:all": "npm run clean:cache && npm run clean:android && rm -rf android/app/build && rm -rf android/.gradle",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:cache": "rm -rf node_modules/.cache && rm -rf /tmp/metro-* && rm -rf /tmp/haste-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-*",
    "build:android:debug": "npm run clean:all && cd android && ./gradlew assembleDebug && cd ..",
    "build:android:release": "npm run clean:all && cd android && ./gradlew assembleRelease && cd ..",
    "build:apk": "npm run build:android:release",
    "logcat": "adb logcat",
    "logcat:react": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "logcat:app": "adb logcat | grep -i 'strack\\|com.strack'",
    "logcat:clear": "adb logcat -c",
    "install:apk:debug": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "install:apk:release": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "install:apk": "npm run install:apk:release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-permissions": "^5.4.4",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.18.0",
    "react-native-svg": "^15.14.0",
    "react-native-yamap": "^4.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
